<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="HastaElMaipo" package="com.sample.hastaElMaipo" version="1.0.0" company="Company Name" />
	<app main="HastaElMaipo" path="Export" file="HastaElMaipo" />
	
	<source path="Source" />
	<window width="720" height="400" fps="60" background="#000000" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" />
	<assets path="Assets/images" rename="images" include="*" />
	<assets path="Assets/sounds" rename="sounds" include="*" />
	<assets path="Assets/fonts" rename="fonts" include="*" />
	
	<icon path="Assets/openfl.svg" />
	
	 <assets path="Assets/sounds" rename="sounds" if="web">
		<sound path="main.mp3" id="main" />
		<sound path="die.mp3" id="die" />
		<sound path="powerup.mp3" id="powerup" />		
		<sound path="fall.mp3" id="fall" />
		<sound path="enemydie.mp3" id="enemydie" />

    </assets>
    <assets path="Assets/sounds" rename="sounds" unless="web">
		<sound path="main.wav" id="main" />
		<sound path="die.ogg" id="die" />
		<sound path="powerup.ogg" id="powerup" />
		<sound path="fall.ogg" id="fall" />
		<sound path="enemydie.ogg" id="enemydie" />
    </assets>

	
</project>