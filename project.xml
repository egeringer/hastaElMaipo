<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="HastaElMaipo" package="com.sample.project" version="1.0.0" company="Company Name" />
	<app main="HastaElMaipo" path="Export" file="HastaElMaipo" />
	
	<source path="Source" />
	<window width="720" height="400" fps="60" background="#000000" />
	<haxelib name="openfl" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" />
	<assets path="Assets/images" rename="images" include="*" />
	<assets path="Assets/sounds" rename="sounds" include="*" />
	
	<icon path="Assets/openfl.svg" />
	
	 <assets path="Assets/sounds" rename="sounds" if="web">
            <sound path="main.mp3" id="main" />
			<sound path="3.mp3" id="tic" />
    </assets>
    <assets path="Assets/sounds" rename="sounds" unless="web">
            <sound path="main.wav" id="main" />
			<sound path="3.wav" id="tic" />
    </assets>

	
</project>